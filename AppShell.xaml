<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Jobbara.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Jobbara"
    xmlns:pages="clr-namespace:Jobbara.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Jobbara">
    
    <ShellContent
    Title="Login"
    ContentTemplate="{DataTemplate pages:inicioSesion}"
    Route="loginPage" />
    <ShellContent 
         Title="userProfile"
        Route="userProfile" 
        ContentTemplate="{DataTemplate pages:userProfile}" />
    <ShellContent
         Title="newChambeador"
        Route="newChambeador" 
        ContentTemplate="{DataTemplate pages:newChambeador}" />
    <ShellContent
        Title="SignUp"
        ContentTemplate="{DataTemplate pages:Registro}"
        Route="signPage" />
    <ShellContent
Title="Home"
ContentTemplate="{DataTemplate pages:homePage}"
Route="homePage" />


</Shell>
